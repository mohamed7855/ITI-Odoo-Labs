<odoo>
    <data>
        <record id='doctors_action' model='ir.actions.act_window'>
            <field name='name'>Doctors</field>
            <field name='res_model'>hms.doctor</field>
        </record>
        
        <!-- Tree View -->
        <record id='iti_doctor_tree_view' model='ir.ui.view'>
            <field name='name'>Doctor Tree View</field>
            <field name='model'>hms.doctor</field>
            <field name='arch' type='xml'>
                <tree>
                    <field name='FirstName'/>
                    <field name='LastName'/>
                </tree>
            </field>
        </record>

        <menuitem name='DOCTORS' id='doctors_menu' parent='iti_main_menu' action='doctors_action' sequence='2'/>
    </data>
</odoo>